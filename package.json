    "nouislider": "^15.7.1",
    "tippy.js": "^6.3.7"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "cssnano": "^6.0.1",
    "eslint": "^8.55.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "terser": "^5.24.0",
    "vite": "^5.4.19"
