{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/calculator",
      "destination": "/index.html"
    },
    {
      "source": "/v5", 
      "destination": "/social_media_cost_calculatorv5.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options", 
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    }
  ]
} 